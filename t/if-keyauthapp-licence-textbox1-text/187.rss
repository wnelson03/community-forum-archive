            <p>You Should Use:</p>
<pre><code>    private void button1_Click(object sender, EventArgs e)
    {
        KeyAuthApp.init();
        KeyAuthApp.license(keyBox.Text);
        if (KeyAuthApp.response.success)
        {
            MainWindow main = new MainWindow();
            main.Show();
            this.Hide();
        }
        else
            MessageBox.Show("Wrong/Used Key!");
    }</code></pre>
